<div class="mb-3">
    <label for="fullname" name="fullname" class="form-label">Full Name</label>
    <input type="text" class="form-control" id="fullname" placeholder="Your Full Name">
    <label for="email" name="email" class="form-label">Email address</label>
    <input type="email" class="form-control" id="email" placeholder="name@example.com">
</div>
<div class="mb-3">
    <label for="message" name="message" class="form-label">Message</label>
    <textarea class="form-control" id="message" rows="5"></textarea>
</div>
<button type="submit" class="btn btn-primary">Submit</button>
